{
  "compilerOptions": {
    "lib": ["deno.ns", "deno.window"],
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "supabase/functions/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src"
  ]
}
